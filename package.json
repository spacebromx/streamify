{
  "name": "streamify",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "babel-jest": "^25.4.0",
    "body-parser": "^1.19.0",
    "bulma": "^0.8.2",
    "classnames": "^2.2.6",
    "components": "^0.1.0",
    "concurrently": "^5.1.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.2",
    "express": "^4.17.1",
    "express-ejs-layouts": "^2.5.0",
    "express-session": "^1.17.1",
    "fetch-jsonp": "^1.1.3",
    "jest-cli": "^25.4.0",
    "jest-transform-stub": "^2.0.0",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-deezer": "^0.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-props": "^0.0.3",
    "react-query": "^1.2.9",
    "react-router-dom": "^5.1.2",
    "scss": "^0.2.4",
    "serve": "^11.3.0"
  },
  "scripts": {
    "build": "NODE_ENV=production parcel build src/index.html --public-url /",
    "watch": "NODE_ENV=development parcel src/index.html",
    "dev": "parcel src/index.html --open",
    "server": "node server.js",
    "start": "concurrently \"yarn run build\" \"yarn run server\"",
    "lint:js": "eslint src/ --ext .js --ext .jsx",
    "lint:js:fix": "eslint src/ --ext .js --ext .jsx --fix",
    "lint:scss": "stylelint 'src/**/*.scss' --syntax scss",
    "lint:scss:fix": "stylelint 'src/**/*.scss' --syntax scss --fix",
    "test:watch": "jest --watch"
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "eslint src --fix"
    ],
    "**/*.scss": [
      "stylelint 'src/**/*.scss' --syntax scss --fix"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {
        "flexbox": "no-2009"
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@types/jest": "^25.2.1",
    "autoprefixer": "^9.7.6",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.4.0",
    "lint-staged": "^10.1.7",
    "node-sass": "^4.14.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "sass": "^1.26.3",
    "stylelint": "^13.3.3",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-selector-bem-pattern": "^2.1.0",
    "typescript": "^3.8.3"
  }
}
